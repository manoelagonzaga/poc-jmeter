                <stringProp name="script">function getAssessmentId() {
	var assessment = vars.get(&quot;assessmentAboutTeam&quot;);
	var teamAssessment = JSON.parse(assessment);
	var assessmentId = teamAssessment.id;
	return assessmentId;
}

function answerAssessment() {
	var assessment = vars.get(&quot;assessmentAboutTeam&quot;);
	var status = vars.get(&quot;evaluationStatus&quot;);
	var teamAssessment = JSON.parse(assessment);
	var answers = teamAssessment.answersConfigurations.answersContent;
	
	teamAssessment.skillItems.forEach( function(skill) {
		skill.skillQuestionItems.forEach( function(question) {
			question.answersIds[0] = teamAssessment.answersConfigurations.answersContent[getRandomAnswer(answers)].id;
		});
	});

	teamAssessment.openQuestionItems.forEach( function(question) {
		question.answer = &quot;This is a load test emulation&quot;
	});

	var assessmentObj = {
		id: teamAssessment.id,
		status: status,
		evaluationCycleId: teamAssessment.evaluationCycleId,
		participantBindId: teamAssessment.participantBindId,
		assessmentItems: {
			skillItems: teamAssessment.skillItems,
			openQuestionItems: teamAssessment.openQuestionItems
		}
	};

	return JSON.stringify(assessmentObj);
}

function getRandomAnswer(answers) {
	min = Math.ceil(0);
	max = Math.floor(answers.length - 1);
	return Math.floor(Math.random() * (max - min + 1)) + min;
}

vars.put(&quot;assessmentId&quot;, getAssessmentId());
vars.put(&quot;answeredTeamAssessment&quot;, answerAssessment());</stringProp>
